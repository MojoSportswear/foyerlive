{
  "name": "transact-theme-vionic",
  "version": "1.0.1",
  "description": "Vionic theme for FoyerLive transact application",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node devServer.js",
    "lint": "eslint ./src",
    "publish": "node publish.js",
    "newpub": "fl-cli --publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foyerlive/transact-theme-vionic.git"
  },
  "keywords": [
    "foyerlive",
    "attract",
    "present",
    "presentation"
  ],
  "author": "James Turle <james@foyerlive.com> (http://github.com/jturle)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foyerlive/transact-theme-vionic/issues"
  },
  "homepage": "https://github.com/foyerlive/transact-theme-vionic#readme",
  "dependencies": {
    "boilerplate-preset": "github:foyerlive/boilerplate-preset",
    "fl-cli": "github:foyerlive/fl-cli"
  }
}
